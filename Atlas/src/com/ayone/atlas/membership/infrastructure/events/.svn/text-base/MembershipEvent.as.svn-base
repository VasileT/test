package com.ayone.atlas.membership.infrastructure.events
{
	
import flash.events.Event;

public class MembershipEvent extends Event
{
	//------------------------------------------------------------
	//
	// Contructor
	//
	//------------------------------------------------------------
	
	public function MembershipEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
	{
		super(type, bubbles, cancelable);
	}
	
	//------------------------------------------------------------
	//
	// Properties
	//
	//------------------------------------------------------------
	
	public static const SHOW_MEMBERS:String = "membershipShowMembers";
	
	public static const MEMBERS_CHANGE:String = "membershipMembersChange";
	
	public static const ADD_CONTACT_TO_ADD_QUEUE:String = "membershipAddContactToAddQueue";
	
	public static const REMOVE_CONTACT_FROM_ADD_QUEUE:String = "membershipRemoveContactFromAddQueue";
	
	public static const CONTACTS_LOADED:String = "membershipContactsLoaded";
}

}