package com.ayone.atlas.session.infrastructure.events
{
	
import flash.events.Event;

public class SessionEvent extends Event
{
	//------------------------------------------------------------
	//
	// Contructor
	//
	//------------------------------------------------------------
	
	public function SessionEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
	{
		super(type, bubbles, cancelable);
	}
	
	//------------------------------------------------------------
	//
	// Properties
	//
	//------------------------------------------------------------
	
	public static const LOGIN_SUCCESFULL:String = "sessionLoginSuccesfull";
	
	public static const LOGIN_FAILED:String = "sessionLoginFailed";
	
	public static const LOGIN_REQUIRED:String = "sessionLoginRequired";
}

}