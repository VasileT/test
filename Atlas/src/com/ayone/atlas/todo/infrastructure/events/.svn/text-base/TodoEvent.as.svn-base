package com.ayone.atlas.todo.infrastructure.events
{
	
import flash.events.Event;

public class TodoEvent extends Event
{
	//------------------------------------------------------------
	//
	// Contructor
	//
	//------------------------------------------------------------
	
	public function TodoEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false)
	{
		super(type, bubbles, cancelable);
	}
	
	//------------------------------------------------------------
	//
	// Properties
	//
	//------------------------------------------------------------
	
	public static const TODOS_CHANGE:String = "todosChange";
	
	public static const TODO_STATUS_CHANGE:String = "todoStatusChange";
}

}